"../../fiftstdlib/Fift.fif" include
"../../fiftstdlib/Asm.fif" include

PROGRAM{
  0  DECLMETHOD stsliceconst
  1  DECLMETHOD stzeroes
  2  DECLMETHOD stskipfirst
  3  DECLMETHOD stsubstr

  stsliceconst PROC:<{
    192 PUSHINT
    NEWC
    8 STU
    b{00010000000000} STSLICECONST
    ENDC
    HASHCU
  }>

  stzeroes PROC:<{
    192 PUSHINT
    NEWC
    8 STU
    6 PUSHINT
    STZEROES
    ENDC
    HASHCU
  }>

  stskipfirst PROC:<{
    5565 PUSHINT // 0b1010110111101, 13 chars
    NEWC
    13 STU
    ENDC
    CTOS
    3 PUSHINT
    SDSKIPFIRST
    HASHSU
  }>

  stsubstr PROC:<{
    5565 PUSHINT // 0b1010110110101, 13 chars
    NEWC
    13 STU
    ENDC
    CTOS
    3 PUSHINT
    4 PUSHINT
    SDSUBSTR // 0b0110
    4 LDU
    DROP
  }>
}END>c