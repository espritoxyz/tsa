  sd_cut_last PROC:<{
    117 PUSHINT // 1110101
    NEWC
    7 STU
    NEWC
    ENDC
    SWAP
    STREF
    ENDC // Now we have a cell with 7 bits equal to 1110101 and one empty ref

    CTOS
    6 PUSHINT
    SDCUTLAST // Expect slice with 6 bits equal to 110101 and no refs, [s]

    DUP // [s s]

    53 PUSHINT
    NEWC
    6 STU
    ENDC
    CTOS

